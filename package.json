    "eslint": "^10.0.2",
    "eslint-config-next": "16.1.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "gh-pages": "^6.3.0",
    "postcss": "^8",
    "tailwindcss": "^4.1.18",
    "typescript": "^5",
    "typescript-eslint": "^8.53.0"
  }
