{
  "name": "justinthelaw",
  "version": "1.0.0",
  "description": "Justin Law's customizable personal website with an AI chatbot",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "dev": "docker compose up -d && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "playwright test",
    "deploy": "npm run build && gh-pages -d out",
    "clean": "docker compose down && rm -rf out test-results playwright-report .next next-env.d.ts node_modules test-results.json",
